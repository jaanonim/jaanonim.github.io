---
import Card from "./Card.astro";

const { repos } = Astro.props;
---

{
    repos.map((repo) => (
        <>
            <Card
                title={repo.name}
                desc={repo.description}
                url={repo.html_url}
                tags={[repo.language]}
                badge={repo.badge}
                page={repo.has_pages}
            />
        </>
    ))
}
